ent);\n\t\t\t\tconst url = \"https://apiyowl.herokuapp.com/api/comments\";\n\t\t\t\tlet requestOptions = {\n\t\t\t\t\tmethod: \"POST\",\n\t\t\t\t\theaders: {\n\t\t\t\t\t\tAccept: \"application/json\",\n\t\t\t\t\t\t\"Content-Type\": \"application/json\",\n\t\t\t\t\t\tAuthorization: \"Bearer \" + this.$store.state.token,\n\t\t\t\t\t},\n\t\t\t\t\tbody: JSON.stringify(comment),\n\t\t\t\t};\n\t\t\t\tawait fetch(url, requestOptions)\n\t\t\t\t\t.then((response) => {\n\t\t\t\t\t\treturn response.json();\n\t\t\t\t\t})\n\t\t\t\t\t.then((response) => {\n\t\t\t\t\t\tconsole.log(response);\n\t\t\t\t\t})\n\t\t\t\t\t.catch((err) => {\n\t\t\t\t\t\tconsole.log(err);\n\t\t\t\t\t});\n\t\t\t\tthis.$router.push(\"/all\");\n\t\t\t},\n\t\t},\n\t};\n</script>\n\n<style scoped>\n\t#rating {\n\t\twidth: 11em;\n\t\tborder: none;\n\t\toutline: none;\n\t\theight: 2em;\n\t}\n\t#cat {\n\t\twidth: auto;\n\t}\n\t#cats {\n\t\twidth: 10em;\n\t\theight: 2em;\n\t\tborder: none;\n\t\toutline: none;\n\t}\n\t#container {\n\t\tmargin-top: 2em;\n\t\tmargin-bottom: 2em;\n\t\tbackground-color: rgb(163, 160, 158);\n\t\twidth: 50%;\n\t\tdisplay: flex;\n\t\tflex-direction: column;\n\t\tpadding: 0.5em;\n\t\tgap: 0.5em;\n\t\tbox-shadow: 1px 1px 3px 1px black;\n\t\twidth: max-content;\n\t}\n\t#check {\n\t\tdisplay: flex;\n\t\tgap: 1em;\n\t}\n\t#but {\n\t\tdisplay: flex;\n\t\twidth: auto;\n\t\tflex-flow: row-reverse;\n\t\tgap: 0.5em;\n\t}\n\t#con {\n\t\tborder: none;\n\t\toutline: none;\n\t\tfont-size: 2em;\n\t\toverflow: hidden;\n\t}\n\t#con::placeholder {\n\t\tfont-size: 1em;\n\t}\n\t#clo {\n\t\tborder: none;\n\t\toutline: none;\n\t\twidth: 5em;\n\t\tbackground-color: red;\n\t}\n\t#add {\n\t\tborder: none;\n\t\toutline: none;\n\t\twidth: 5em;\n\t\theight: 2em;\n\t}\n</style>"],"sourceRoot":""}]}