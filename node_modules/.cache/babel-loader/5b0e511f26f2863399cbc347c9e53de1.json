Ae,IAAf;AAPc,iBANP;AAAA;AAAA,uBAeR,KAAK,CAAC,GAAD,EAAM,cAAN,CAAL,CACJ,IADI,CACC,UAAC,QAAD,EAAc;AACnB,yBAAO,QAAQ,CAAC,IAAT,EAAP;AACA,iBAHI,EAIJ,IAJI,CAIC,UAAC,QAAD,EAAc;AACnB,kBAAA,KAAI,CAAC,OAAL,CAAa,KAAb,GAAqB,QAAQ,CAAC,KAA9B;AACA,kBAAA,EAAE,CAAC,SAAH,GAAe,KAAI,CAAC,OAAL,CAAa,KAAb,CAAmB,MAAlC;AACA,iBAPI,EAQJ,KARI,CAQE,UAAC,GAAD,EAAS;AACf,kBAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;AACA,iBAVI,CAfQ;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA0Bd;AA3BO;AAZK,CAAf","sourcesContent":["<template>\n\t<div id=\"com\">\n\t\t<h2 style=\"font-weight: bold\">{{ comment.user.name }}</h2>\n\t\t<h4>{{ comment.content }}</h4>\n\t\t<div style=\"display: flex; gap: 5em\">\n\t\t\t<p style=\"color: rgb(255, 0, 0)\">\n\t\t\t\t{{ comment.rating }} &nbsp;<img\n\t\t\t\t\tsrc=\"../assets/Star.png\"\n\t\t\t\t\tstyle=\"width: 20px; padding-bottom: 7px\"\n\t\t\t\t/>\n\t\t\t</p>\n\t\t\t<div style=\"color: blue\">\n\t\t\t\t<span id=\"ll\">{{ comment.likes.length }}</span>\n\t\t\t\t&nbsp;\n\t\t\t\t<img\n\t\t\t\t\tstyle=\"width: 20px; padding-bottom: 7px\"\n\t\t\t\t\tsrc=\"https://img.icons8.com/external-sbts2018-blue-sbts2018/58/000000/external-like-social-media-basic-1-sbts2018-blue-sbts2018.png\"\n\t\t\t\t/>\n\t\t\t</div>\n\t\t\t<p role=\"button\" style=\"color: blue\" @click=\"like(comment.id)\">I likes</p>\n\t\t\t<p>{{ ago }}</p>\n\t\t</div>\n\t</div>\n</template>\n\n<script>\n\timport moment from \"moment\";\n\texport default {\n\t\tname: \"ComLink\",\n\t\tprops: [\"comment\"],\n\t\tdata() {\n\t\t\treturn {};\n\t\t},\n\n\t\tcomputed: {\n\t\t\tago() {\n\t\t\t\treturn moment(this.comment.created_at).fromNow();\n\t\t\t},\n\t\t},\n\t\tmethods: {\n\t\t\tasync like(id) {\n\t\t\t\tconst url = \"https://apiyowl.herokuapp.com/api/likes\";\n\t\t\t\tlet like = {\n\t\t\t\t\tcomment_id: id,\n\t\t\t\t};\n\t\t\t\tconst ll = document.querySelector(\"#ll\");\n\t\t\t\tlet requestOptions = {\n\t\t\t\t\tmethod: \"POST\",\n\t\t\t\t\theaders: {\n\t\t\t\t\t\tAccept: \"application/json\",\n\t\t\t\t\t\t\"Content-Type\": \"application/json\",\n\t\t\t\t\t\tAuthorization: \"Bearer \" + this.$store.state.token,\n\t\t\t\t\t},\n\t\t\t\t\tbody: JSON.stringify(like),\n\t\t\t\t};\n\t\t\t\tawait fetch(url, requestOptions)\n\t\t\t\t\t.then((response) => {\n\t\t\t\t\t\treturn response.json();\n\t\t\t\t\t})\n\t\t\t\t\t.then((response) => {\n\t\t\t\t\t\tthis.comment.likes = response.likes;\n\t\t\t\t\t\tll.innerHTML = this.comment.likes.length;\n\t\t\t\t\t})\n\t\t\t\t\t.catch((err) => {\n\t\t\t\t\t\tconsole.log(err);\n\t\t\t\t\t});\n\t\t\t},\n\t\t},\n\t};\n</script>\n\n<style>\n\t#com {\n\t\tborder: none;\n\t\tdisplay: flex;\n\t\tflex-direction: column;\n\t\tbox-shadow: 1px 1px 2px 1px rgb(216, 211, 211);\n\t\tmargin-bottom: 15px;\n\t\tpadding: 15px;\n\t\tborder-radius: 5px;\n\t\tbackground: rgb(207, 207, 207);\n\t\twidows: auto;\n\t}\n</style>"],"sourceRoot":""}]}